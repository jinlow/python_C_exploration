   "source": [
    "x = [1, 2, 3, 4, 5]\n",
    "y = [1, 2, 3, 4, 5]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [],
   "source": [
    "b0 = 0\n",
    "b1 = 0\n",
    "alpha = 0.05\n",
    "epoch = 100\n",
    "for i in range(0,len(x)*epoch):\n",
    "    idx = i % len(x)\n",
    "    p = b0 + b1 * x[idx]\n",
    "    err = p - y[idx]\n",
    "    b0 = b0 - alpha * err\n",
    "    b1 = b1 - alpha * err * x[idx]\n"
   ]
  },
